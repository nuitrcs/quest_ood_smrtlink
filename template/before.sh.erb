# Export the module function if it exists
[[ $(type -t module) == "function" ]] && export -f module

# Find available port to run server on
port=$(find_port localhost 7000 11000 )
export SMRTLINK_GUI_PORT=$port
export SMRTLINK_SERVICES_PORT=$(find_port localhost 7000 11000 )
export SMRTLINK_CROMWELL_PORT=$(find_port localhost 7000 11000 )
export SMRTLINK_DATABASE_PORT=$(find_port localhost 7000 11000 )
